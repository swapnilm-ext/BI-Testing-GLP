              usesDispositions="true"
              version="7"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7">
              <properties>
                <property
                  name="CommandTimeout"
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out.">0</property>
                <property
                  name="OpenRowset"
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset.">[dbo].[STG_dimPatients]</property>
                <property
                  name="OpenRowsetVariable"
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."></property>
                <property
                  name="SqlCommand"
                  dataType="System.String"
                  description="The SQL command to be executed."
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT
    CAST(p_id AS numeric) p_id,
    Name,
	ClinicName,
    state,
    city,
    contact,
    ZipCode,
    Gender,
    Dob1.yyyy +'/'+ Dob1.mm +'/'+ Replace(Dob1.dd,' ',0) AS DOB,
    NextOfKin ,
	Case when  Mg_Data like '%[%$#@^&amp;*&gt;&lt;?()]%' then 1 else 0 end FieldChar_Check,
	Mg_Data,
	CASE when ISDATE (Dob1.yyyy +'/'+ Dob1.mm +'/'+ Replace(Dob1.dd,' ',0)) = 1 then 1 else 0 end InvalidDate_check,
	GETDATE() as startdate
  FROM STG_dimPatients Pat

  INNER JOIN (SELECT
    CAST(p_id AS int) p_id1,
	'Patient:-'+Cast(P_ID as varchar(20)) +'  Name:-'+Name+'  State:-'+State+'  City:-'+City+'   Zipcode:-'+ZipCode as Mg_Data,
    REVERSE(SUBSTRING(REVERSE(dob), 1, 4)) AS yyyy,
    REVERSE(SUBSTRING(REVERSE(dob), 9, 2)) AS dd,
    CASE
      WHEN CHARINDEX('January', dob) = 1 THEN '01'
      WHEN CHARINDEX('February', dob) = 1 THEN '02'
      WHEN CHARINDEX('March', dob) = 1 THEN '03'
      WHEN CHARINDEX('April', dob) = 1 THEN '04'
      WHEN CHARINDEX('May', dob) = 1 THEN '05'
      WHEN CHARINDEX('June', dob) = 1 THEN '06'
      WHEN CHARINDEX('July', dob) = 1 THEN '07'
      WHEN CHARINDEX('August', dob) = 1 THEN '08'
      WHEN CHARINDEX('September', dob) = 1 THEN '09'
      WHEN CHARINDEX('October', dob) = 1 THEN '10'
      WHEN CHARINDEX('November', dob) = 1 THEN '11'
      WHEN CHARINDEX('December', dob) = 1 THEN '12'
    END mm
  FROM STG_dimPatients) Dob1
    ON Pat.p_id = Dob1.p_id1</property>
